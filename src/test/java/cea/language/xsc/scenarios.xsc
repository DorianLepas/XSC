// SCENARIOS

VFEI_SECS_SEQUENCES:
<L
    <L INITIALIZE
        <A 'CR: S1F13'>
        <A 'DR_CLEAR: S2F33 W * Clear all reports' >
        <A 'LER_CLEAR: S2F35 W * Unlink all event reports' >
        <A 'DR_MEASUREMENT: S2F33 W * Define Report for MEASUREMENT'>
        <A 'LER_MEASUREMENT: S2F35 W * Link Event Report for DR_MEASUREMENT' >
        //<A 'EDER_ENABLE_ALL: S2F37 W * Enable all event reports' >
        <A 'EDER_INIT_MEASUREMENT: S2F37 W * Enable event reports for MEASUREMENT' >
        <A 'EAS_ENABLE_ALL: S5F3 W * Enable all alarms' >
        <A 'HCS_ONLINE_LOCAL: S2F49 W * Host command send Go Local' >
    >

    <L GO_REMOTE
        <A 'HCS_ONLINE_REMOTE: S2F49'>
    >

    <L GO_LOCAL
        <A 'HCS_ONLINE_LOCAL: S2F49'>
    >

    <L ECR
        <A 'ECNR_ALL: S2F29'>
        <A 'ALL_ECR: S2F13 W * ALL equipment constants request with zero-length list'>
    >

    <L ESVR
        <A 'SVNR_ALL: S1F11'>
        <A 'ALL_SSR: S1F3 W * Status queries=> Selected all equipment status request'>
    >
    <L EDVR
        <A 'EDVR_ALL: S1F21'>
    >
    <L CER
        <A 'CER_ALL: S1F23'>
    >
    <L INIT_MINIMUM
        <A 'CR: S1F13'>
        <A 'DR_CLEAR: S2F33 W * Clear all reports' >
        <A 'LER_CLEAR: S2F35 W * Unlink all event reports' >
        <A 'EDER_ENABLE_ALL: S2F37 W * Enable all event reports' >
        <A 'HCS_ONLINE_LOCAL: S2F49 W * Host command send Go LOCAL' >
    >

    <L INIT_REPORT_LOCAL_REMOTE
        <A 'CR: S1F13'>
        <A 'DR_CLEAR: S2F33 W * Clear all reports' >
        <A 'LER_CLEAR: S2F35 W * Unlink all event reports' >
        <A 'DR_INIT_LR: S2F33 W * Define Reports LOCAL_REMOTE' >
        <A 'LER_INIT_LR: S2F35 W * Link Report LOCAL_REMOTE' >
        <A 'EDER_INIT_LR: S2F37 W * Enable event reports LOCAL_REMOTE' >
        <A 'HCS_ONLINE_LOCAL: S2F49 W * Host command send Go Local' >
        <A 'HCS_ONLINE_REMOTE: S2F49 W * Host command send Go Remote' >
    >

     <L INIT_REPORT_MEASURMENT
        <A 'CR: S1F13'>
        <A 'DR_CLEAR: S2F33 W * Clear all reports' >
        <A 'LER_CLEAR: S2F35 W * Unlink all event reports' >
        <A 'DR_MEASUREMENT: S2F33 W * Define Report for MEASUREMENT'>
        <A 'LER_MEASUREMENT: S2F35 W * Link Event Report for DR_MEASUREMENT' >
        <A 'EDER_ENABLE_ALL: S2F37 W * Enable all event reports' >
        <A 'HCS_ONLINE_LOCAL: S2F49 W * Host command send Go Local' >
    >

     <L INIT_REPORT_FDC_ONLYEVENTFDC
        <A 'CR: S1F13'>
        <A 'DR_CLEAR: S2F33 W * Clear all reports' >
        <A 'LER_CLEAR: S2F35 W * Unlink all event reports' >
        <A 'TRACE_FDC_JOURNALIERE:S2F23 W SVID demandées pour monitoring FDC'>
        <A 'TRACE_FDC_HEBDOMADAIRE:S2F23 W SVID demandées pour monitoring FDC' >
        <A 'DR_FDC: S2F33 W * Define Report for FDC'>
        <A 'LER_FDC: S2F35 W * Link Event Report for FDC' >
        <A 'EDER_INIT_FDC: S2F37 W * Enable event reports FDC' >
        <A 'HCS_ONLINE_LOCAL: S2F49 W * Host command send Go Local' >
    >

    <L INIT_REPORT_TRACE_FDC
        <A 'TRACE_FDC_JOURNALIERE:S2F23 W SVID demandées pour monitoring FDC'>
        <A 'TRACE_FDC_HEBDOMADAIRE:S2F23 W SVID demandées pour monitoring FDC' >
        <A 'TRACE_FDC_POURTEST:S2F23 W * comme aucuns event lié à la FDC ne remonte... on met toutes les variables dans une trace ; )' >
    >

    <L INIT_REPORT_FDC
        <A 'DR_FDC: S2F33 W * Define Report for FDC'>
        <A 'LER_FDC: S2F35 W * Link Event Report for FDC' >
    >

>
.

